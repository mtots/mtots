{
    "files.eol": "\n",
    "files.associations": {
        "*.h": "c",
        "typeinfo": "c",
        "__hash_table": "c",
        "__split_buffer": "c",
        "array": "c",
        "bitset": "c",
        "initializer_list": "c",
        "unordered_map": "c",
        "utility": "c",
        "vector": "c",
        "string": "c",
        "string_view": "c",
        "__bit_reference": "c",
        "__functional_base": "c",
        "__node_handle": "c",
        "algorithm": "c",
        "atomic": "c",
        "chrono": "c",
        "cstddef": "c",
        "__memory": "c",
        "functional": "c",
        "iterator": "c",
        "limits": "c",
        "locale": "c",
        "optional": "c",
        "ratio": "c",
        "system_error": "c",
        "tuple": "c",
        "type_traits": "c",
        "memory": "c",
        "__functional_03": "c",
        "__locale": "c",
        "__string": "c",
        "istream": "c",
        "__functional_base_03": "c",
        "__tuple": "c",
        "__threading_support": "c",
        "new": "c",
        "__config": "c",
        "ios": "c",
        "iosfwd": "c",
        "random": "cpp",
        "ostream": "c",
        "cstdlib": "cpp",
        "sstream": "c"
    },
    "C_Cpp.default.defines": [
        "FT2_BUILD_LIBRARY=1",
        "MTOTS_ENABLE_GG=1",
        "MTOTS_ENABLE_AUDIO=1",
        "MTOTS_ENABLE_IMAGE=1",
        "MTOTS_ENABLE_CANVAS=1",
        "MTOTS_ENABLE_ZIP=1",
        "MTOTS_ENABLE_FONT=1",
        "MTOTS_ENABLE_FONT_ROBOTO_MONO=1",
        "MTOTS_ENABLE_PACO=1",
        // "MTOTS_ENABLE_OPENGLES3=1",
        "MTOTS_USE_PRINTFLIKE=1"
    ],
    "cmake.configureOnOpen": false,
    "C_Cpp.default.mergeConfigurations": false,
    "C_Cpp.default.enableConfigurationSquiggles": true,
    "[c]": {
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.tabSize": 2
    },
    "python.analysis.typeCheckingMode": "strict"
}
