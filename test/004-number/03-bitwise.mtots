
print('AND')

print("3 & 1 = %r" % [3 & 1])
print("-1 & 10 = %r" % [-1 & 10])
print("-2 & 13 = %r" % [-2 & 13])
print("16 & 8 = %r" % [16 & 8])
print("12 & 14 = %r" % [12 & 14])

print("-1 & -1 = %r" % [-1 & -1])

print('OR')

print("3 | 1 = %r" % [3 | 1])
print("-1 | 10 = %r" % [-1 | 10])
print("-2 | 13 = %r" % [-2 | 13])
print("16 | 8 = %r" % [16 | 8])
print("12 | 14 = %r" % [12 | 14])

print("-1 | -1 = %r" % [-1 | -1])

print('XOR')

print("3 ^ 1 = %r" % [3 ^ 1])
print("-1 ^ 10 = %r" % [-1 ^ 10])
print("-2 ^ 13 = %r" % [-2 ^ 13])
print("16 ^ 8 = %r" % [16 ^ 8])
print("12 ^ 14 = %r" % [12 ^ 14])

print("-1 ^ -1 = %r" % [-1 ^ -1])

print('NOT')

print("~1 = %r" % [~1])
print("~-1 = %r" % [~-1])
print("~0 = %r" % [~0])
print("~3 = %r" % [~3])

print('SHIFT')

print("2 >> 1 = %r" % [2 >> 1])
print("2 << 1 = %r" % [2 << 1])
print("1 << 3 = %r" % [1 << 3])
print("100 >> 3 = %r" % [100 >> 3])

print("-1 >> 0 = %r" % [-1 >> 0])
print("-1 >> 1 = %r" % [-1 >> 1])
print("-1 >> 30 = %r" % [-1 >> 30])

print("-1 << 0 = %r" % [-1 << 0])
print("-1 << 1 = %r" % [-1 << 1])
print("-1 << 31 = %r" % [-1 << 31])
